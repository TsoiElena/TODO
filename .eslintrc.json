{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb"],
  "rules": {
    "semi": "off",
    "no-console": "warn",
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": ["error", 150],
    "no-tabs": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "indent": "off",
    "no-param-reassign": "off",
    "linebreak-style": "off"
  }
}
